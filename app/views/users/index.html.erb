<table class="table">
	<thead>
		<th>#</th>
		<th>Name</th>
		<th>Email</th>
		<th></th>
		<th></th>
	</thead>
	<tbody>
		<% @users.each_with_index do |user, index| %>
			<tr>
				<td><%=index + 1%></td>
				<td><%=user.name%></td>
				<td><%=user.email%></td>
				<td><%=link_to "Edit", edit_user_path(user.id) %></td>
				<td><%=link_to "Destroy", user_path(user.id), method: :delete %></td>
			</tr>
		<% end %>
	</tbody>
</table>
<%=link_to "New User", new_user_path, class: "btn btn-primary" %>